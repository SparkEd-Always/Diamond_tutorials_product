{
  "expo": {
    "name": "AVM Tutorial Teacher",
    "slug": "avm-tutorial-teacher",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.avmtutorial.teacher",
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "This app uses notifications to send you important updates from your school."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4F46E5"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.avmtutorial.teacher",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "notification": {
      "color": "#4F46E5",
      "androidMode": "default",
      "androidCollapsedTitle": "AVM Tutorial"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "https://avm-tutorial-api.railway.app",
      "eas": {
        "projectId": "avm-tutorial-teacher"
      }
    }
  }
}
