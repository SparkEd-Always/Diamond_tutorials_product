{
  "expo": {
    "name": "Sparky",
    "slug": "sparky",
    "owner": "koustubsk",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C4E6B"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sparked.sparky",
      "buildNumber": "2",
      "infoPlist": {
        "NSUserNotificationsUsageDescription": "This app uses notifications to send you important updates from your school."
      }
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sparked.sparky",
      "permissions": [
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.INTERNET",
        "android.permission.VIBRATE"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "color": "#2C4E6B",
      "androidMode": "default",
      "androidCollapsedTitle": "Sparky"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "apiUrl": "http://192.168.1.4:8000",
      "eas": {
        "projectId": "d9867ba4-c8ce-4ab6-9408-58606c1bfab7"
      }
    }
  }
}